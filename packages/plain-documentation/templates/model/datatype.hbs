<span class="model-property-type">
    {{#if discriminator}}<span class="type">Discriminator</span>{{/if}}
    
    {{#if anchor}}
        {{#isObject}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId nativeType}}">{{nativeType}}</a>
        {{else isMap}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId componentNativeType}}">{{nativeType}}</a>
        {{else isArray}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId componentNativeType}}">{{nativeType}}</a>
        {{else isEnum}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId nativeType}}">{{nativeType}}</a>
        {{else}}
            {{nativeType}}
            {{#if nativeType.wireType}}
            ({{nativeType.wireType}})
            {{/if}}
        {{/isObject}}
    {{else}}
        {{nativeType}}
    {{/if}}
</span>
{{#if default}}
    <span class="model-property-default-value">{{default}}</span>
{{/if}}
