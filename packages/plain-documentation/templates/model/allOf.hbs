{{!
renders a model "allOf"-definition.
}}
<section class="model-allOf">
    {{! Display ref (i.e. superclasses)}}
    <section class="model-allOf-additional">
        {{#each .}}
            {{#if $ref}}
                {{#resolve_ref $ref}}
                    {{>model/inherited}}
                {{/resolve_ref}}
            {{/if}}
            {{#unless $ref}}
                {{>model/body}}
            {{/unless}}
        {{/each}}
    </section>
</section>