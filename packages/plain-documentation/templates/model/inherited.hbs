{{!
renders the inherited properties of a parent class.
}}

<div class="parent">
    <h5>Parent</h5>
    <p><a class="model-ref" href="#{{anchor}}/{{htmlId nativeType}}">{{nativeType}}</a></p>

    {{#if properties}}
        <div class="params -inherited-properties">
            <h5>Inherited Properties</h5>
            <table>
                {{>model/frag/inherited}}
            </table>
        </div>
    {{/if}}
</div>
