{{!
    Renders a model inside a bootstrap-panel.
    @public
    @readonly
}}

                <section class="summary" data-class-title="{{name}}">
                    {{#if name}}
                        {{#if anchor}}
                            <h4 class="api-title"><a name="{{anchor}}/{{htmlId nativeType}}"></a>{{nativeType}} {{#each allOf}}{{#if $ref}}<span class="type">&lt; {{>model/reference $ref}}</span>{{/if}}{{/each}}</h4>
                        {{else}}
                            <h4 class="api-title">{{nativeType}}</h4>
                        {{/if}}
                    {{/if}}

                    {{>model/body anchor=anchor}}

                    {{#models}}
                        {{>model/main-panel anchor=../anchor}}
                    {{/models}}
                </section>
