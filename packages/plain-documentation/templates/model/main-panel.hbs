{{!
    Renders a model inside a bootstrap-panel.
    @public
    @readonly
}}

                <section class="summary" data-class-title="{{name}}">
                    {{#if name}}
                        {{#if anchor}}
                            <h4><a name="{{anchor}}/{{htmlId nativeType}}"></a>{{nativeType}} {{#each allOf}}{{#if $ref}}<span class="type">&lt; {{>model/reference $ref}}</span>{{/if}}{{/each}}</h4>
                        {{else}}
                            <h4>{{nativeType}}</h4>
                        {{/if}}
                    {{/if}}

                    {{#if parent}}
                    <div class="parent">
                        <h5>Parent</h5>
                        <p><a class="model-ref" href="#{{anchor}}/{{htmlId parent}}">{{parent}}</a></p>
                    {{/if}}

                    {{>model/body anchor=anchor}}

                    {{#models}}
                        {{>model/main-panel anchor=../anchor}}
                    {{/models}}
                </section>
